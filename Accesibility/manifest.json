{
    "name": "Accessible reading",
    "version": "0.0.0.2",
    "manifest_version": 3,
    "description": "An extension that makes the web more readable through several accessibility options.",
        "icons": {
        "16": "icons/accesibility16x16.png",
        "32": "icons/accesibility32x32.png",
        "48": "icons/accesibility48x48.png",
        "128": "icons/accesibility128x128.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": ["activeTab", "scripting"]

}